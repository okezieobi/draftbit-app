{
  "name": "draftbit-app",
  "version": "0.0.0",
  "main": "dist/www/firebase.js",
  "directories": {},
  "scripts": {
    "dev": "DEBUG=draftbit-app:* nodemon --exec babel-node src/bin/www.js",
    "release": "npm run clean && npm run build && npm run deploy",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-app",
    "build-app": "babel src -s -d dist --presets=@babel/env",
    "emulator": "firebase emulators:start --only functions",
    "deploy": "npm firebase deploy --only functions"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.1",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongo": "latest",
    "mongoose": "latest",
    "morgan": "~1.9.1",
    "regenerator-runtime": "^0.13.7",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-tools": "^8.16.2",
    "nodemon": "^2.0.4"
  }
}
